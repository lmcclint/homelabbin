# Synology Container Services Environment Variables
# Copy this file to .env and set your own secure values
# NEVER commit the actual .env file to git!

# Harbor Container Registry
HARBOR_CORE_SECRET=changeme-random-32-char-string
HARBOR_JOBSERVICE_SECRET=changeme-another-random-string
HARBOR_DB_PASSWORD=changeme-secure-harbor-db-password

# Gitea Git Repository
GITEA_DB_PASSWORD=changeme-secure-gitea-db-password

# Splunk Enterprise
SPLUNK_ADMIN_PASSWORD=changeme-secure-splunk-password
SPLUNK_HEC_TOKEN=changeme-uuid-format-token

# Network Configuration
# These should match your VLAN 60 configuration
HARBOR_IP=192.168.60.10
NEXUS_IP=192.168.60.11
GITEA_IP=192.168.60.12
SPLUNK_IP=192.168.60.15

# DNS Names (add to your DNS server)
# harbor.lab.local -> 192.168.60.10
# nexus.lab.local -> 192.168.60.11
# git.lab.local -> 192.168.60.12
# splunk.lab.local -> 192.168.60.15

